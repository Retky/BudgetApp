<div>
  <% @categories.each do |cat| %>
    <div><%= cat.icon %></div>
    <div>
      <div>
        <%= link_to cat.name, movements_path(category_id: cat.id), method: :get %>
      </div>
      <div>$ <%= cat.balance %></div>
    </div>
  <% end %>
</div>
<div class="greenBtn">
  <%= button_to 'New Category', new_category_path, method: :get, class: 'newBtn' %>
</div>
